<%- if controller_name != 'sessions' %>
  <%= link_to "Entrar", new_session_path(resource_name), class: "link-dark" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  Não tem uma conta?
  <%= link_to "Cadastre-se", new_registration_path(resource_name), class: "link-dark" %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Esqueceu a senha?", new_password_path(resource_name), class: "link-dark" %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Não recebeu instruções para confirmação?", new_confirmation_path(resource_name), class: "link-dark" %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Não recebeu instruções para desbloqueio?", new_unlock_path(resource_name), class: "link-dark" %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Entrar com #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), method: :post, class: "link-dark" %><br />
  <% end %>
<% end %>